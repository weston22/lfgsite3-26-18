function initForm(){
 
   var qstring = window.location.search;
   qstring = qstring.substring(1,qstring.length);
   var newqstring = "";
   if ((qstring != "") && (qstring!="logoff")) {
      qstring = qstring.split("&");
      for(i = 0; i < qstring.length; i++) {
         var qpair = qstring[i].split("=");
         if(qpair.length > 0) {
            if(qpair[0] != "language" && qpair[0] != "country") {
               if(newqstring.length > 0) {
                  newqstring += "&";
               }
               newqstring += qstring[i];
            }
         }
      }
   }
   document.myForm2.qstring.value = newqstring;
   document.myForm2.userid.focus();
}


function ValidateLogIn(){
    var go = ValidateId();

    if (go) go = ValidateLogPass();
    if (go) document.myForm2.submit();
}

function ValidateId(){ 
      if((document.myForm2.userid.value == "")||
       (document.myForm2.userid.value.charAt(0) == " ")){
        alert(invalid_user_number);
		
        document.myForm2.userid.focus();
		document.myForm2.userid.select();       
        return false;
    }
    return true;    
}

function ValidateLogPass(){
    if (document.myForm2.password.value == ""){
        alert(invalid_password);
        document.myForm2.password.focus();
        document.myForm2.password.select();
        return false;
    } 
    return true;    
}

function open_window(mypage,title,w,h,scroll){
   winl=(screen.width - w)/2
   wint=(screen.height - h)/2
   winprops = 'height='+h+',width='+w+',left='+winl+',top='+wint+',resizable=no,scrollbars='+scroll
   win = window.open(mypage,title,winprops)
   if (parseInt(navigator.appVersion) >= 4) { win.window.focus(); }
}

